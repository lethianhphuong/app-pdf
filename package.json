{
  "name": "gt-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build-manual": "vite build",
    "build": "npm run precommit && cmd /c build-production.bat",
    "login": "cmd /c login.vbs",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint --ext .tsx,.ts,.js src",
    "lint:fix": "eslint --fix src --ext ts,tsx",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prepare": "husky install",
    "clean:cache": "rimraf .eslintcache && rimraf node_modules && npm install",
    "precommit": "tsc --noEmit --skipLibCheck && eslint --ext .tsx,.ts,.js src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^2.0.3",
    "@ant-design/icons": "^5.3.1",
    "@ant-design/pro-components": "^2.7.0",
